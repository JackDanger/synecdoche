---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>Haiku Bible</title>
 <link href="http://synecdoche.com/atom.xml" rel="self"/>
 <link href="http://synecdoche.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://synecdoche.com/</id>
 <author>
   <name>Jack Danger Canty</name>
   <email>synecdoche@jackcanty.com</email>
 </author>
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://synecdoche.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://synecdoche.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
</feed>
